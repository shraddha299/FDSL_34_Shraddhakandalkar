#include<stdio.h>
int main()
{
  int n,i,j,key,found=0;
  printf("Enter size of array:");
  scanf("%d",&n);
  int a[n];
  printf("\nEnter %d array elements:",n);
  for(i=0;i<n;i++) 
 {
  scanf("%d",&a[i]);
 }
  printf("\nEnter a Element for search:");
  scanf("%d",&key);
  for(i=0;i<n;i++)
  {
       if(a[i]==key)
      { 
           printf("Element found at position %d\n",i+1);
           found=1;
         break;
   }
}

   if(found==0)
   {
       printf("Element not found\n");
   }
return 0;
  }
