#include<stdio.h>
#define size 5
int rear=-1,front=-1;
int queue[size];
void enQueue(int element)
{
   if(rear==-1 && front==-1)
   {
     rear=0;
     front=0;
     queue[rear]=element;
     printf("Inserted element is %d\n",element);
   }
   else if((rear+1)%size==front)
   {
     printf("Queue is full\n");
   }
   else
   {
     rear=(rear+1)%size;
     queue[rear]=element;
     printf("Inserted element is %d\n",element);
   }
}
void deQueue()
{
  if (rear==-1 && front==-1)
  {
    printf("Queue is empty\n");
  }
  else if(rear==front)
  {
    printf("element deleted is %d\n",queue[front]);
    rear=-1;
    front=-1;
  }
  else
  {
    printf("deleted element is %d\n",queue[front]);
    front=(front+1)%size;
  }
}

void display()
{
  if(rear==-1 && front==-1)
  {
    printf("Queue is empty\n");
  }
  else
 {
   printf("Queue elements are:\n");
   for(int i=front;i<=rear;i++)
   {
     printf("%d ",queue[i]);
   }
 }
}

int main()
{  
  
  enQueue(7);
  enQueue(8);
  enQueue(9);
  enQueue(3);
  enQueue(5);
  enQueue(6);
  display();
  deQueue();
  deQueue();
  deQueue();
  display();
return 0;
}
